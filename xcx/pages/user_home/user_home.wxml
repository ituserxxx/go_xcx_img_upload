<view wx:for="{{img_list}}" wx:key="idi" ws:for-item="item" class="img">
  <view>
    <text>{{item.create_time}}</text>
  </view>
  <view class="img_list">
    <view wx:for="{{item.pic_list}}" wx:key="idj" ws:for-item="item" >
      <van-image width="100" height="100" mode='cover' src="{{item.img_url}}"  class="img_item" data-item="{{item}}" bind:click="preview" />
    </view>
  </view>
</view>